    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter Web (Chrome)",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "chrome"
            ]
        },
        
        {
            "name": "croatia_explorer",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "croatia_explorer (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "croatia_explorer (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}